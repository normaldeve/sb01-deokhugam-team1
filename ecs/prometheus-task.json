{
  "family": "prometheus-task",
  "networkMode": "bridge",
  "executionRoleArn": "arn:aws:iam::135808941684:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::135808941684:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "prometheus",
      "image": "public.ecr.aws/b0y2f2j8/prometheus:with-config",
      "cpu": 128,
      "memory": 256,
      "memoryReservation": 128,
      "portMappings": [
        {
          "containerPort": 9090,
          "hostPort": 9090,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/prometheus-task",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "256",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
